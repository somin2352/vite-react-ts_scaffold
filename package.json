{
  "private": true,
  "type": "module",
  "name": "likelion-vite-react",
  "version": "1.0.0",
  "description": "Vite 빌드 도구를 사용해 React, CSS, Zustand, React-Router를 활용한 내향인들을 위한 취미 프로젝트",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=9.0.0",
    "vscode": ">=1.9.0"
  },
  "scripts": {
    "start": "pnpm dev --open",
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --cache \"src/**/*.{ts,tsx,js}\"",
    "format": "prettier --write --cache 'src/**/*.{ts,tsx}'",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "pnpm lint",
      "pnpm format"
    ]
  },
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "9.22.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "supabase": "2.19.7",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1",
    "vite": "6.2.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.49.1",
    "clsx": "2.1.1",
    "gsap": "3.12.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "5.0.0",
    "react-router": "7.3.0",
    "react-textarea-autosize": "8.5.8",
    "sweetalert2": "11.17.2",
    "zustand": "5.0.3"
  }
}
